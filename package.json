{
  "name": "@gjfleo/eslint-config",
  "type": "module",
  "version": "1.0.19",
  "description": "GjfLeo's ESLint config.",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/gjfleo/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gjfleo/eslint-config.git"
  },
  "bugs": "https://github.com/gjfleo/eslint-config/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@antfu/eslint-config": ">=4.14.0",
    "eslint": ">=9.5.0",
    "eslint-plugin-format": ">=0.1.0"
  },
  "dependencies": {
    "local-pkg": "^1.1.1"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.2",
    "@antfu/utils": "^0.7.10",
    "@eslint/eslintrc": "^3.3.1",
    "@gjfleo/eslint-config": "workspace:*",
    "@types/eslint__eslintrc": "^2.1.2",
    "@types/node": "^20.17.47",
    "bumpp": "^9.11.1",
    "esno": "^4.8.0",
    "lint-staged": "^15.5.2",
    "pnpm": "^9.15.9",
    "rimraf": "^6.0.1",
    "simple-git-hooks": "^2.13.0",
    "typescript": "^5.8.3",
    "unbuild": "^2.0.0",
    "vite": "^5.4.19",
    "vitest": "^2.1.9"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
